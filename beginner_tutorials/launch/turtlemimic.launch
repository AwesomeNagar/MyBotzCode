<launch>

<group ns = "tutorialsim1">
<node pkg = "turtlesim" name = "sim" type = "turtlesim_node"/>
</group>

<group ns = "tutorialsim2">
<node pkg = "turtlesim" name = "sim" type = "turtlesim_node"/>
</group>

<node pkg = "turtlesim" name = "mimic" type = "mimic">
<remap from = "input" to="tutorialsim1/turtle1"/>
<remap from = "output" to="tutorialsim2/turtle1"/>
</node>
</launch>
